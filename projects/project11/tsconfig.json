{
  "extends": "@tsconfig/node14" /* from node_modules */,
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "module": "commonjs",
    "strictNullChecks": false,
    "noImplicitAny": false,
    "baseUrl": "./src",
    "paths": {
      "myproject/*": ["./*"],
      "custom/*": ["custom_modules/*"],
      "extra": ["../../project2/index"]
    },
    "resolveJsonModule": true
  },
  "exclude": ["node_modules"]
}
